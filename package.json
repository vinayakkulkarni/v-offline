{
    "name": "v-offline",
    "description": "Offline and Online components for VueJS",
    "version": "1.0.4",
    "author": {
        "name": "Vinayak Kulkarni",
        "email": "vinayak.kulkarni@live.com",
        "url": "https://vinayak.website"
    },
    "private": false,
    "main": "dist/v-offline.min.js",
    "module": "dist/v-offline.esm.js",
    "keywords": [
        "vuejs",
        "offline",
        "online",
        "detect",
        "internet-connectivity",
        "disconnect"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vinayakkulkarni/v-offline.git"
    },
    "scripts": {
        "test": "npm run bundle && karma start karma.conf.js",
        "test-watch": "karma start karma.conf.js --no-single-run",
        "build": "npm run bundle && npm run bundle:min",
        "bundle": "rollup -c build/rollup.conf.js",
        "bundle:min": "rollup -c build/rollup.min.conf.js"
    },
    "devDependencies": {
        "aliasify": "^2.1.0",
        "babel-preset-es2015": "^6.18.0",
        "babelify": "^7.3.0",
        "browserify": "^13.3.0",
        "jasmine-core": "^2.5.2",
        "karma": "^1.3.0",
        "karma-browserify": "^5.1.0",
        "karma-jasmine": "^1.1.0",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-spec-reporter": "^0.0.26",
        "phantomjs-prebuilt": "^2.1.14",
        "rollup": "^0.54.1",
        "rollup-plugin-babel": "^3.0.3",
        "rollup-plugin-uglify": "^2.0.1",
        "rollup-plugin-vue": "^3.0.0",
        "vue": "^2.5.13",
        "vue-template-compiler": "^2.5.13",
        "watchify": "^3.8.0"
    },
    "licenses": [
        {
            "type": "MIT",
