{
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "vueIndentScriptAndStyle": true,
  "ignorePatterns": [
    ".nuxt",
    ".output",
    "dist",
    "node_modules",
    ".wrangler",
    "coverage",
    "*.min.js",
    "*.min.css",
    "bun.lock",
    "**/CHANGELOG.md",
    "**/jsr.json"
  ]
}
