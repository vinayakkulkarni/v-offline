{
    "name": "v-offline",
    "description": "Offline and Online components for VueJS",
    "version": "1.1.0",
    "author": {
        "name": "Vinayak Kulkarni",
        "email": "vinayak.kulkarni@live.com",
        "url": "https://vinayak.website"
    },
    "private": false,
    "main": "dist/v-offline.min.js",
    "module": "dist/v-offline.esm.js",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vuejs",
        "offline",
        "online",
        "detect",
        "internet-connectivity",
        "disconnect"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vinayakkulkarni/v-offline.git"
    },
    "scripts": {
        "test": "npm run bundle && karma start karma.conf.js",
        "test-watch": "karma start karma.conf.js --no-single-run",
        "build": "npm run bundle && npm run bundle:min",
        "bundle": "rollup -c build/rollup.conf.js",
        "bundle:min": "rollup -c build/rollup.min.conf.js"
    },
    "devDependencies": {
        "aliasify": "^2.1.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-preset-env": "^1.7.0",
        "babel-loader": "^8.0.5",
        "babelify": "^8.0.0",
        "browserify": "^16.2.3",
        "eslint": "^5.15.2",
        "eslint-plugin-vue": "^6.1.2",
        "jasmine-core": "^3.3.0",
        "karma": "^4.0.1",
        "karma-browserify": "^5.3.0",
        "karma-jasmine": "^1.1.2",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-spec-reporter": "^0.0.32",
        "phantomjs-prebuilt": "^2.1.16",
        "rollup": "^0.59.4",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-commonjs": "^9.2.1",
        "rollup-plugin-uglify": "^3.0.0",
        "rollup-plugin-vue": "^4.7.2",
        "vue": "^2.6.9",
        "vue-template-compiler": "^2.6.9",
        "watchify": "^3.11.1"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vinayakkulkarni/v-offline/issues"
    },
    "homepage": "https://we-online.site/"
}
