{
  "name": "v-offline",
  "version": "3.2.1",
  "description": "Offline and Online components for Vue",
  "type": "module",
  "types": "types/index.d.ts",
  "main": "dist/v-offline.cjs",
  "module": "dist/v-offline.js",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./dist/v-offline.js",
      "require": "./dist/v-offline.umd.cjs"
    }
  },
  "scripts": {
    "bundle": "npm run build && npm run build:dts",
    "build": "vite build",
    "build:dts": "rimraf ./types && vue-tsc --declaration --emitDeclarationOnly --outDir ./types && prettier --write ./types/**/*.d.ts",
    "test": "echo 'test!'",
    "prepare": "husky install",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:css",
    "lintfix": "npm run lint:prettier:fix && npm run lint:eslint:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint './src/**/*.{cjs,mjs,js,ts,vue}' --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix './src/**/*.{cjs,mjs,js,ts,vue}' --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|example|shims|dist)/**/}*.{cjs,mjs,js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|example|shims|dist)/**/}*.{cjs,mjs,js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint \"{,!(node_modules|example|shims|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix \"{,!(node_modules|example|shims|dist)/**/}*.{css,scss,vue}\" --ignore-path .gitignore",
    "release": "shipjs prepare",
    "release:auto": "shipjs prepare --yes",
    "release:dry": "shipjs prepare --dry-run"
  },
  "dependencies": {
    "ping.js": "^0.3.0"
  },
  "peerDependencies": {
    "ping.js": "^0.3.0",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.8.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/runtime-dom": "^3.2.40",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "shipjs": "^0.24.4",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vue": "^3.2.40",
    "vue-tsc": "^0.40.13"
  },
  "keywords": [
    "vuejs",
    "offline",
    "online",
    "detect",
    "internet-connectivity",
    "disconnect"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-offline.git"
  },
  "private": false,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-offline/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-offline#readme"
}
